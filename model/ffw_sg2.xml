<mujoco model="ffw_sg2">
  <compiler angle="radian" meshdir="assets" autolimits="true"/>

  <option integrator="implicitfast"/>

  <!--
    Default classes:
    1) "robot" for main body joints (position control)
    2) "robot_torque" for torque-controlled joints (MPC)
    3) "gripper" for the gripper joints
    4) "slider" specifically for the linear slide joint
  -->
  <default>
    <!-- Robot default (position control) -->
    <default class="robot">
      <joint range="-3.14 3.14" armature="0.5" damping="100.0" />
      <position kp="3000" forcerange="-1000 1000" ctrlrange="-3.14 3.14"/>
    </default>

    <!-- Robot torque control (for MPC-controlled joints) -->
    <default class="robot_torque">
      <joint range="-3.14 3.14" armature="0.5" damping="50.0" />
      <motor gear="1" ctrlrange="-100 100" ctrllimited="true"/>
    </default>

    <!-- Hand (gripper) default -->
    <default class="gripper">
      <joint armature="0.1" damping="10.0"/>
      <position kp="1000" forcerange="-100 100"/>
    </default>

    <!-- Slider default (for the lift_joint) -->
    <default class="slider">
      <joint type="slide" range="-0.5 0.0" armature="10.0" damping="5000.0" frictionloss="100.0" />
      <position kp="10000" forcerange="-100000 100000" ctrlrange="-0.5 0.0"/>
    </default>
  </default>

